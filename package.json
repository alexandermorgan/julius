{
    "name": "julius",
    "description": "Web application for formalized score control.",
    "version": "0.0.2",
    "author": "Christopher Antila <christopher@antila.ca>",
    "license": "GPL-3.0",
    "repository": "https://jameson.adjectivenoun.ca:ncoda/julius.git",
    "main": "js/electron_main.js",
    "dependencies": {
        "amazeui-react": "1.0.1",
        "codemirror": "5.13.2",
        "electron-prebuilt": "0.37.2",
        "history": "2.0.1",
        "nuclear-js": "1.3.0",
        "react": "0.14.7",
        "react-custom-scrollbars": "3.1.0",
        "react-dom": "0.14.7",
        "react-router": "2.0.1",
        "react-split-pane": "0.1.25"
    },
    "devDependencies": {
        "babelify": "7.2.0",
        "babel-jest": "9.0.3",
        "babel-plugin-add-module-exports": "0.1.2",
        "babel-plugin-syntax-object-rest-spread": "^6.3.13",
        "babel-plugin-transform-object-rest-spread": "^6.3.13",
        "babel-preset-es2015": "6.5.0",
        "babel-preset-es2015": "6.6.0",
        "babel-preset-react": "6.5.0",
        "eslint": "2.4.0",
        "eslint-plugin-react": "4.2.3",
        "jest-cli": "0.9.2",
        "less": "2.6.1",
        "less-plugin-clean-css": "1.5.1",
        "react-addons-test-utils": "0.14.7",
        "watchify": "3.7.0"
    },
    "browserify": {
        "transform": [
            "babelify"
        ]
    },
    "scripts": {
        "start": "python3 devserver",
        "test": "jest"
    },
    "jest": {
        "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
        "testFileExtensions": [
            "es6",
            "js"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "es6"
        ],
        "unmockedModulePathPatterns": [
            "<rootDir>/node_modules/react",
            "<rootDir>/node_modules/react-dom",
            "<rootDir>/node_modules/react-addons-test-utils",
            "<rootDir>/node_modules/nuclear-js",
            "<rootDir>/js/nuclear",
            "<rootDir>/js/nuclear/stores"
        ],
        "testDirectoryName": "tests",
        "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js"
    },
    "babel": {
        "presets": [
            "es2015",
            "react"
        ],
        "plugins": [
            "transform-object-rest-spread"
        ],
        "comments": true,
        "compact": false
    }
}
